var = 645

add_Loop: 
	adc #1            ;   test
	bcs sub_Loop     
	sta 0             
	jmp add_Loop       

sub_Loop:
	sub #1          
	sta %0        
	beq add_Loop      
	jmp sub_Loop       

interrupt:
	inc 2
	jsr print
	rti

print:
    lda msg,rX
    beq stop
    inx 
    out
    jmp (print)

stop:
	rts

msg:
	.tx    "Hello World"
	.db    'c'
	.db 0

.org $fffd
irq:
	jmp interrupt

